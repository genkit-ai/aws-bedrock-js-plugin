{
  "name": "genkitx-aws-lambda-example",
  "version": "1.0.0",
  "description": "Example AWS Lambda function using Genkit with AWS Bedrock",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "genkit:start": "cross-env GENKIT_ENV=dev genkit start -- tsx --watch src/index.ts",
    "dev": "serverless offline --httpPort 3000",
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "info": "serverless info",
    "logs": "serverless logs -f storyGenerator -t",
    "remove": "serverless remove"
  },
  "keywords": [
    "genkit",
    "aws",
    "lambda",
    "bedrock",
    "ai"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "genkit": "^1.28.0",
    "genkitx-bedrock": "file:../../"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/node": "^22.5.4",
    "cross-env": "^7.0.3",
    "serverless": "^4.6.0",
    "serverless-offline": "^14.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.6.2"
  }
}
